{
  "name": "X PANEL",
  "description": "Degub Tool For React Developers",
  "version": "1.0",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "icons":{
    "16":"icon16.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon":{
      "16":"icon16.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions":["activeTab","scripting","tabs","webNavigation"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "http://127.0.0.1/*",
        "https://127.0.0.1/*",
        "http://localhost/*",
        "https://localhost/*"
      ]
    }
  ]
}
